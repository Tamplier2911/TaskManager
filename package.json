{
  "name": "taskmanager",
  "version": "1.0.0",
  "description": "Never stop learning",
  "main": "index.js",
  "engines": {
    "node": "12.9.1",
    "npm": "6.11.2"
  },
  "scripts": {
    "client": "cd client && npm run dev",
    "server": "nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "start": "node index.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.3",
    "express-sslify": "^1.2.0",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.22",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.11",
    "sharp": "^0.25.4",
    "validator": "^13.1.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  },
  "author": "Artem Nikolaiev",
  "license": "ISC"
}
